		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@playwright/test": "^1.49.1",
		"@types/node": "^22.10.6",
		"concurrently": "^9.1.2"
