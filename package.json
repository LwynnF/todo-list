{
	"name": "todo-list",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@fortawesome/free-solid-svg-icons": "^6.7.2",
		"@fortawesome/react-fontawesome": "^0.2.2",
		"axios": "^1.7.9",
		"cors": "^2.8.5",
		"dotenv": "^16.4.7",
		"pg": "^8.13.1",
